{
  "name": "milestones",
  "version": "1.0.0",
  "description": "A Nuxt3 front-end application for tracking personal milestones!",
  "private": true,
  "author": {
    "name": "anirbmuk",
    "email": "anirbmuk@live.in"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anirbmuk/nuxt-milestones.git"
  },
  "bugs": {
    "url": "https://github.com/anirbmuk/nuxt-milestones/issues"
  },
  "homepage": "https://home-milestones.firebaseapp.com",
  "keywords": [
    "mongodb",
    "express",
    "nuxt",
    "vue",
    "node"
  ],
  "scripts": {
    "build": "nuxt build",
    "clean": "rimraf .nuxt && rimraf .output",
